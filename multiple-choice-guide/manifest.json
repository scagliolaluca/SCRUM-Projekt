{
  "manifest_version": 3,
  "name": "Multiple-Choice Guide",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "permissions": [
    "activeTab"
  ],
  "icons": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
 },
  "keywords": [
    "keyword1",
    "keyword2",
    "keyword3",
    "to"
  ]
}
