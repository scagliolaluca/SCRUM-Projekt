{
  "manifest_version": 3,
  "name": "Multiple-Choice Guide",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "permissions": [
    "activeTab"
  ],
  "icons": {
      "16": "images/favicon-16x16.png",
      "32": "images/favicon-32x32.png",
      "150": "images/favicon-150x150.png",
      "384": "images/android-chrome-384x384.png"
 },
  "keywords": [
    "keyword1",
    "keyword2",
    "keyword3",
    "to"
  ]
}
